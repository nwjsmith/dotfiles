[alias]
  st = status
  co = checkout
  dc = diff --cached --color
  di = diff --color
  unstage = reset -- 
  l = log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(green) %an, %ar%Creset'
[core]
  excludesfile = $HOME/.gitignore
  editor = vim
  trustctime = false
[push]
  default = current
[color]
  ui = true
[mergetool "fugitive"]
  cmd = vim -f -c \"Gdiff\" \"$MERGED\"
[merge]
  tool = fugitive
  ff = only
[mergetool]
  keepBackup = false
[include]
  path = ~/.gitconfig.local
[commit]
  template = ~/.gitmessage
[pull]
  rebase = true
[url "https://github.com/"]
  insteadOf = git://github.com/
  insteadOf = git@github.com:
  insteadOf = github:
