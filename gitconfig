[absorb]
  maxStack = 200
[alias]
  co = checkout
  ctags = !.git/hooks/ctags
  dc = diff --cached --word-diff=color
  di = diff --word-diff=color
  l = log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(green) %an, %ar%Creset'
  st = status
  unstage = reset -- 
  yolo = push --force-with-lease
  push-each = !git rev-list HEAD...master | xargs -n1 -I{} git push origin {}:$(git symbolic-ref HEAD)-{}
  clean-each = !git ls-remote --quiet --heads origin $(git symbolic-ref HEAD)-* | cut -f2 | xargs -n1 -I{} git push origin :{}
[branch]
  autoSetupMerge = true
[core]
  excludesfile = ~/.gitignore
  editor = vim
  trustctime = false
[push]
  default = current
[pull]
  rebase = true
[color]
  ui = true
[mergetool "fugitive"]
  cmd = vim --nofork -c \"Gdiff\" \"$MERGED\"
[merge]
  renamelimit = 10000
  tool = fugitive
[mergetool]
  keepBackup = false
[include]
  path = ~/.gitconfig.platform
  path = ~/.gitconfig.local
[init]
  templateDir = ~/.git-template
[commit]
  # gpgsign = true
  template = ~/.gitmessage
  verbose = true
[user]
  email = nwjsmith@gmail.com
  name = Nate Smith
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[rebase]
  autoStash = true
[github]
  user = nwjsmith
[magithub]
  online = false
[magithub "status"]
  includeStatusHeader = false
  includePullRequestsSection = false
  includeIssuesSection = false
