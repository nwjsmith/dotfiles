[alias]
  st = status
  co = checkout
  dc = diff --cached --color
  di = diff --color
  unstage = reset -- 
  l = log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(green) %an, %ar%Creset'
  yolo = push --force-with-lease
  ctags = !.git/hooks/ctags
  rim = rebase --interactive master
  rc = rebase --continue
[core]
  excludesfile = /Users/nwjsmith/.gitignore
  editor = vim
  trustctime = false
[push]
  default = current
[pull]
  rebase = true
[color]
  ui = true
[mergetool "fugitive"]
  cmd = vim --nofork -c \"Gdiff\" \"$MERGED\"
[merge]
  tool = fugitive
  renamelimit = 10000
[mergetool]
  keepBackup = false
[include]
  path = ~/.gitconfig.platform
  path = ~/.gitconfig.local
[init]
  templateDir = ~/.git-template
[commit]
  template = ~/.gitmessage
  gpgsign = true
  verbose = true
[user]
  name = Nate Smith
  email = nwjsmith@gmail.com
[diff]
  compactionHeuristic = true
